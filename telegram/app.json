{
  "expo": {
    "name": "Telegram",
    "slug": "telegram",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
   "ios": {
  "supportsTablet": true,
  "bundleIdentifier": "com.kartiks.telegram",
  "infoPlist": {
    "CFBundleURLTypes": [
      {
       
        "CFBundleURLSchemes": ["com.googleusercontent.apps.106379162499-ulrbuu02lv40p98a8hveui4aakdm8hgc"]
      }
    ]
  }
},
 "android": {
      "googleServicesFile":"./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kartiks.telegram",
      "permission": ["android.permission.RECORD_AUDIO"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",{
        "iosUrlScheme": "com.googleusercontent.apps.106379162499-ulrbuu02lv40p98a8hveui4aakdm8hgc"
      }
    ],
    ["expo-image-picker",
      {
        "photosPermission": "Allow $(PRODUCT_NAME) to access your photos",
        "cameraPermission": "Allow $(PRODUCT_NAME) to open the camera",
        "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone"
      }
    ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPath":true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e16ea018-885e-4386-9718-9be93be2d426"
      }
    }
  }
}
